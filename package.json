{
    "name": "remitter-portal",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "ng serve -c local --aot",
        "proxy": "ng lint --force && ng serve -c local --aot --proxy-config proxy.conf.json --port 4201",
        "install-proxy": "npm ci && ng lint --force && ng serve -c local --aot --proxy-config proxy.conf.json --port 4201",
        "proxy-no-aot": "ng lint --force && ng serve -c local --proxy-config proxy.conf.json --port 4201",
        "build": "ng build --prod",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "purge": "rm -rf node_modules && rm -rf dist && npm cache verify && npm i",
        "build-dev": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build -c dev --base-href=/remitter-portal/",
        "build-prod": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --prod --base-href=/remitter-portal/"
    },
    "private": true,
    "dependencies": {
        "@angular-devkit/build-angular": "^0.800.2",
        "@angular/animations": "8.0.0",
        "@angular/cdk": "^8.0.1",
        "@angular/common": "8.0.0",
        "@angular/compiler": "8.0.0",
        "@angular/core": "8.0.0",
        "@angular/flex-layout": "^8.0.0-beta.26",
        "@angular/forms": "8.0.0",
        "@angular/http": "7.2.15",
        "@angular/material": "^8.0.1",
        "@angular/material-moment-adapter": "^8.0.1",
        "@angular/platform-browser": "8.0.0",
        "@angular/platform-browser-dynamic": "8.0.0",
        "@angular/router": "8.0.0",
        "@ng-idle/core": "^7.0.0-beta.1",
        "@sentry/browser": "^5.4.0",
        "backoff-rxjs": "^6.1.2",
        "classlist.js": "^1.1.20150312",
        "core-js": "^3.1.3",
        "file-saver": "^2.0.2",
        "hammerjs": "^2.0.8",
        "keycloak-angular": "^6.1.0",
        "material-design-icons": "^3.0.1",
        "moment-timezone": "^0.5.25",
        "ngx-cacheable": "^1.2.3",
        "ngx-markdown": "^8.2.1",
        "normalize.css": "^8.0.1",
        "postscribe": "^2.0.8",
        "rxjs": "^6.5.2",
        "sentry": "^0.1.2",
        "uuid": "^3.3.2",
        "web-animations-js": "^2.3.1",
        "zone.js": "^0.9.1"
    },
    "devDependencies": {
        "@angular/cli": "^8.0.2",
        "@angular/compiler-cli": "8.0.0",
        "@angular/language-service": "8.0.0",
        "@types/file-saver": "^2.0.1",
        "@types/jasmine": "^3.3.13",
        "@types/jasminewd2": "^2.0.6",
        "@types/node": "^12.0.8",
        "@types/uuid": "^3.4.4",
        "angular-file": "^1.2.2",
        "codelyzer": "^5.1.0",
        "jasmine-core": "~3.4.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "^4.1.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "^2.0.5",
        "karma-jasmine": "^2.0.1",
        "karma-jasmine-html-reporter": "^1.4.2",
        "protractor": "^5.4.2",
        "ts-node": "~8.2.0",
        "tslint": "~5.17.0",
        "typescript": "3.4.5"
    }
}
